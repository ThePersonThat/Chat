<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="background" stylesheets="@style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.Controller">
   <children>
      <TextArea fx:id="Output" layoutX="14.0" layoutY="14.0" prefHeight="311.0" prefWidth="571.0" styleClass="background" stylesheets="@style.css" wrapText="true">
         <font>
            <Font size="19.0" />
         </font>
      </TextArea>
      <TextField fx:id="Input" layoutX="14.0" layoutY="338.0" prefHeight="32.0" prefWidth="504.0" styleClass="input-area" stylesheets="@style.css">
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor>
      </TextField>
      <Button fx:id="Send" contentDisplay="CENTER" graphicTextGap="10.0" layoutX="534.0" layoutY="338.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="35.0" stylesheets="@style.css" textAlignment="CENTER">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font />
         </font>
      </Button>
      <TitledPane animated="false" prefHeight="27.0" prefWidth="600.0" text="untitled">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
        </content>
      </TitledPane>
   </children>
</AnchorPane>
